{
    "name": "burger-queen-api",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "pretest": "eslint .",
        "start": "nodemon index.js",
        "test:e2e": "jest --coverage --verbose --testPathPattern e2e --globalSetup ./e2e/globalSetup --globalTeardown ./e2e/globalTeardown --runInBand",
        "test:unit": "jest --coverage --verbose --testPathPattern unit",
        "test": "npm run test:e2e && npm run test:unit",
        "lint": "eslint .",
        "docs:generate": "jsdoc -c jsdoc.conf --readme README-docs.md",
        "docs:deploy": "rm -rf docs && npm run docs:generate && gh-pages -d docs"
    },
    "dependencies": {
        "bcrypt": "^3.0.6",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.6.9",
        "nodemon": "^1.19.1"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-transform-modules-commonjs": "^7.2.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^24.8.0",
        "docdash": "^1.1.1",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.17.2",
        "gh-pages": "^2.0.1",
        "jest": "^24.7.1 ",
        "jsdoc": "^3.6.2",
        "jsdoc-http-plugin": "^0.3.0",
        "mongodb-memory-server": "^5.1.9",
        "node-fetch": "^2.3.0",
        "supertest": "^4.0.2",
        "tree-kill": "^1.2.1"
    }
}
